    "bluebird": "^3.7.2",
    "bull": "^4.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "date-fns": "^2.28.0",
    "dotenv": "^12.0.3",
    "express": "^4.17.2",
    "graphql": "15",
    "graphql-scalars": "^1.14.1",
    "handlebars": "^4.7.7",
    "helmet": "^5.0.1",
    "ioredis": "^4.28.3",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "marked": "^4.0.10",
    "mongo-seeding": "^3.7.1",
    "mongoose": "^6.1.6",
    "nodemailer": "^6.7.2",
    "prom-client": "^14.0.1",
